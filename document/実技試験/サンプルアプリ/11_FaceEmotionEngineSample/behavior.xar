                                                        <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.memory = ALProxy("ALMemory")
        self.periodic_task = qi.PeriodicTask()
        self.periodic_task.setName("CheckMemoryKeys")
        self.periodic_task.setCallback(self.checkMemoryKeys)
        self.periodic_task.setUsPeriod(self.getParameter("periodLoop")*1000)
        pass

    def onUnload(self):
        self.periodic_task.stop()

        pass

    def onInput_onStart(self):
        self.periodic_task.start(False)
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        #self.onStopped() #activate the output of the box


    def checkMemoryKeys(self):
        trackedId = self.memory.getData("ALBasicAwareness/HumanTracked")
        if (trackedId == -1):
            return
        try:
            expressionProperties = self.memory.getData("PeoplePerception/Person/"+str(trackedId)+"/ExpressionProperties")
            maxValue = max(expressionProperties)
            index = expressionProperties.index(maxValue)

            if index == 0:
                self.isNeutral()

            elif index == 1:
                if (expressionProperties[1]/(1-expressionProperties[0]) > 0.7):
                    self.takePicture()
                    smileValue = self.memory.getData("PeoplePerception/Person/"+str(trackedId)+"/SmileProperties")
                    smileValue[0] = int(smileValue[0] * 100)
                    self.memory.insertData("sbm_sample/smileValue", smileValue[0])
                    self.logger.info(expressionProperties)
                    self.isHappy()

            elif index == 2:
                if (expressionProperties[2]/(1-expressionProperties[0]) > 0.7):
                    self.takePicture()
                    value = int(expressionProperties[2]/(1-expressionProperties[0])*100)
                    self.memory.insertData("sbm_sample/smileValue", value)
                    self.logger.info(expressionProperties)
                    self.isSurprised()

            elif index == 3:
                if (expressionProperties[3]/(1-expressionProperties[0]) > 0.4):
                    self.takePicture()
                    value = int(expressionProperties[3]/(1-expressionProperties[0])*100)
                    self.memory.insertData("sbm_sample/smileValue", value)
                    self.logger.info(expressionProperties)
                    self.isAngry()

            elif index == 4:
                if (expressionProperties[4]/(1-expressionProperties[0]) > 0.8):
                    self.takePicture()
                    value = int(expressionProperties[4]/(1-expressionProperties[0])*100)
                    self.memory.insertData("sbm_sample/smileValue", value)
                    self.logger.info(expressionProperties)
                    self.isSad()

        except:
            return


        if (len(expressionProperties) != 5):
            return

#        if (expressionProperties[4]/(1-expressionProperties[0]) > self.getParameter("threshold")):
#            self.isFrowning()]]>
</content>
