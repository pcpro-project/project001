        if (self.isSubscribed):
            self.facecharacteristics.unsubscribe(self.getName())
            self.isSubscribed = False
        pass

    def onInput_onStart(self):
        if (not self.isSubscribed):
            self.isSubscribed = True
            self.facecharacteristics.subscribe(self.getName(), self.getParameter("period"), 0)
